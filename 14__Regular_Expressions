string = "search inside of this text please!"

print('search' in string)                                   # True

print(re.search('this', string))                            # <re.Match object; span=(17, 21), match='this'>
                                                            # give an object, the word starts from index 17 and ends at 21 (not included)

a = re.search('this', string)                                                            
print(a.span())                                             # (17, 21)
print(a.start())                                            # 17
print(a.end())                                              # 21
print(a.group())                                            # this  |  gives the part of the string that was a match

a = re.search('thIs', string)
#print(a.group())                                            # AttributeError: 'NoneType' object has no attribute 'group'







import re

pattern = re.compile('this')
string = 'search this inside of this text please!'

a = pattern.search(string)
print(a.group())                                            # this

b = pattern.findall(string)
print(b)                                                    # ['this', 'this']

c = pattern.fullmatch(string)
print(c)                                                    # None

pattern = re.compile('search this inside of this text please!')
c = pattern.fullmatch(string)
print(c)                                                    # <re.Match object; span=(0, 39), match='search this inside of this text please!'>


pattern = re.compile('search')
string = 'search this inside of this text please!'
d = pattern.match(string)
print(d)                                                    # <re.Match object; span=(0, 6), match='search'>


pattern = re.compile('this')
d = pattern.match(string)
print(d)     






import re

pattern = re.compile(r"([a-zA-z]).([a])")                   # r" means raw string
string = 'Hey how are you'

a = pattern.search(string)
print(a.group(1))                                            # w
print(a.group(2))                                            # s
print(a.group())                                             # w a






import re

regex_pattern = re.compile(r"([A-Za-z0-9$%#@]{8,}\d)")

password = 'dfsd34$%dfs6'
check = regex_pattern.fullmatch(password)
print(check)                                                        # <re.Match object; span=(0, 12), match='dfsd34$%dfs6'>


